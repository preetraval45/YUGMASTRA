{
  "name": "yugmastra",
  "version": "0.1.0",
  "private": true,
  "description": "Autonomous Adversary-Defender Co-Evolution Platform",
  "keywords": [
    "cybersecurity",
    "ai",
    "reinforcement-learning",
    "machine-learning",
    "red-team",
    "blue-team",
    "autonomous-systems"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/yugmastra.git"
  },
  "license": "MIT",
  "author": "Your Name",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "@turbo/gen": "^2.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.2.5"
}
